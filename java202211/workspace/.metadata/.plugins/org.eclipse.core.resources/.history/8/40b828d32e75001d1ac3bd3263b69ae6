package com.tests;

public abstract class Employee {

	//생성자
	public Employee() {
	
	}
	
	
	
	public Employee(String name, int number, String department, int salary) {
		super();
		this.name = name;
		Number = number;
		this.department = department;
		this.salary = salary;
	}



	//field
	private String name;
	private int Number;
	private String department;
	private int salary;
	
	//method
	public double tax() {
		return 0;
	}
	
	
	
}//
