package sampleproj;

class ThreadEx_10_1 extends Thread {
	@Override
	public void run() {
		for(int i=0; i<100; i++) {
			System.out.print("-");
		}
	}
}//

class ThreadEx_10_2 extends Thread {

	@Override
	public void run() {
		for(int i=0; i<100; i++) {
			System.out.print("|");
		}

	}
}//

public class ThreadEx_10 {

	public static void main(String[] args) {
		
	}
	
}//
