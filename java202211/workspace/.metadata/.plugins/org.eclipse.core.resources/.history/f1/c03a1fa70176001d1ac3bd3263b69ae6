package sampleproj;


class ThreadEx_05_1 extends Thread {
	@Override
	public void run() {
		for(int i=0; i<300; i++) {
		System.out.println("-");
		}
		System.out.println("<< Thread 1 종료 >>");
	}
}

class ThreadEx_05_2 extends Thread {
	@Override
	public void run() {
	
	}
}


