package com.test;

import java.util.HashMap;

public class MapTest {
	
	public static void main(String[] args) {
		
		HashMap<String, String> map = new HashMap<String, String>();
		
		// Map에 데이터를 저장할 때
		map.put("123", "Hello");
		map.put("4", "안녕!");
		
		//Map에서 데이터를 추출할 때
		System.out.println(map.get("123"));
		
	}
	
}//
