package com.test;

class MyClass extends Exception {
	
}

public class CustomeExceptionTest {

	public static void main(String[] args) {
		
		try {
			
			throw new MyClass();	//exception 객체가 생성됨 (아직은 그냥 객체 단계)
									//이런 exception객체를 던져야 함 => throw
		} catch(Exception e) {
			
		}
		System.out.println("끝");
		
	}
	
}//
